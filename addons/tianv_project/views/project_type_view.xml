<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>
        <record id="view_tianv_project_deduct_type_tree" model="ir.ui.view">
            <field name="name">view.tianv.project.deduct.type.tree</field>
            <field name="model">tianv.project.deduct.type</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name"/>
                    <field name="commission_type"/>
                </tree>
            </field>
        </record>
        <record id="view_tianv_project_deduct_type_form" model="ir.ui.view">
            <field name="name">view.tianv.project.deduct.type.form</field>
            <field name="model">tianv.project.deduct.type</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <group>
                                <field name="name"/>
                                <field name="commission_type"/>
                            </group>
                            <group>
                                <group attrs="{'invisible':[('commission_type','!=','employee')]}">
                                    <field name="commission_code"/>
                                </group>
                                <group attrs="{'invisible':[('commission_type','!=','account')]}">
                                    <field name="journal_id"/>
                                    <field name="debit_account_id" domain="[('type','!=','view')]"/>
                                    <field name="credit_account_id" domain="[('type','!=','view')]"/>
                                </group>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

    </data>
</openerp>