                  groups="tianv_project.group_project_admin"/>

        <menuitem id="menu_project_template" name="项目模板" parent="tianv_service.menu_service_config" sequence="10"
                  action="action_project_template"
                  groups="tianv_project.group_project_admin"/>

        <menuitem id="menu_project_deduct_type" name="项目提成类型" parent="tianv_service.menu_service_config" sequence="20"
                  action="action_project_deduct_type"
                  groups="tianv_project.group_project_admin"/>

        <act_window id="action_project_order_creation"
