<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>
        <record id="action_project_deduct_type" model="ir.actions.act_window">
            <field name="name">项目提成类型</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">tianv.project.deduct.type</field>
            <field name="view_mode">tree,form</field>
        </record>

        <record id="action_project_project" model="ir.actions.act_window">
