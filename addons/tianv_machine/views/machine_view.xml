<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <record id="view_attendance_machine_tree" model="ir.ui.view">
            <field name="name">view.tianv.attendance.machine.tree</field>
            <field name="model">tianv.attendance.machine</field>
            <field name="arch" type="xml">
                <tree editable="top">
                    <field name="code"/>
                    <field name="user_id"/>
                    <field name="user_true_name"/>
                    <field name="log_employee"/>
                    <field name="log_time"/>
                </tree>
            </field>
        </record>

        <record id="view_attendance_machine_log_tree" model="ir.ui.view">
            <field name="name">view.tianv.attendance.machine.log.tree</field>
            <field name="model">tianv.attendance.machine.log</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="import_datetime"/>
                    <field name="is_success"/>
                    <field name="error_info"/>
                </tree>
            </field>
        </record>
    </data>
</openerp>