<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <record id="view_attendance_machine_tree" model="ir.ui.view">
            <field name="name">view.tianv.attendance.machine.tree</field>
            <field name="model">tianv.attendance.machine</field>
            <field name="arch" type="xml">
                <tree editable="top">
                    <field name="id" readonly="1"/>
                    <field name="user_true_name"/>
                    <field name="log_employee"/>
                    <field name="log_time"/>
                    <button name="button_match_all_employee" type="object" string="匹配记录" groups="tianv_machine.group_attendance_machine_admin"/>
                    <button name="button_remove_all_record" type="object" string="删除所有记录" groups="tianv_machine.group_attendance_machine_admin"
                            confirm="确定删除所有记录？删除的记录不能被恢复"/>
                </tree>
            </field>
        </record>

        <record id="view_attendance_machine_search" model="ir.ui.view">
            <field name="name">view.tianv.attendance.machine.search</field>
            <field name="model">tianv.attendance.machine</field>
            <field name="arch" type="xml">
                <search>
                    <field name="user_true_name"/>
                    <field name="log_employee"/>
                    <field name="log_time"/>
                </search>
            </field>
        </record>

        <record id="view_attendance_machine_log_tree" model="ir.ui.view">
            <field name="name">view.tianv.attendance.machine.log.tree</field>
            <field name="model">tianv.attendance.machine.log</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="import_datetime"/>
                    <field name="is_success"/>
                    <field name="error_info"/>
                </tree>
            </field>
        </record>
    </data>
</openerp>