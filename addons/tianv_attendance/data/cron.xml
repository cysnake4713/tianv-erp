<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data noupdate="1">
        <record model="ir.cron" id="insurance_cron">
            <field name="name">考勤自动生成</field>
            <field name="interval_number">1</field>
            <field name="interval_type">months</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
            <field name="model" eval="'tianv.hr.attendance.plan'"/>
            <field name="function" eval="'cron_generate_record'"/>
            <field name="args" eval="'()'"/>
        </record>

        <record id="wechat_map_tianv_attendance" model="odoosoft.wechat.enterprise.map">
            <field name="code">tianv_attendance.wechat_code</field>
        </record>

        <record id="message_template_attendance" model="odoosoft.wechat.enterprise.message.template">
            <field name="name">微信考勤审批提醒</field>
            <field name="title">考勤审批提醒</field>
            <field name="content">
员工: ${object.employee.name}
会计周期: ${object.period.name}
状态: ${fields['state'].convert_to_export(object.state, env)}
            </field>
        </record>
    </data>
</openerp>